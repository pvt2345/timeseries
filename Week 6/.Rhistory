d = 1
D = 0
per = 9
n = round(length(ts_TCB_Close)*4/5)
for (p in 1:3){
for(q in 1:2){
for(i in 1:1){
for(j in 1:2){
model<-arima(x=ts_TCB_Close, order = c((p-1),d,(q-1)), seasonal = list(order=c((i-1),D,(j-1)), period=per), optim.method="BFGS")
